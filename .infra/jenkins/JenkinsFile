pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                powershell '''
                    echo "in build"
                '''
            }
        }
        stage('Test')
        {
            steps {
                powershell'''
                    echo "in test"
                '''
            }
        }
        stage('Static Code Analysis')
        {
            steps {
                posershell'''
                    echo "I am in Sonarqube scan"
                '''
            }
        }
        stage('Build DockerImage')
        {
            steps {
                powershell'''
                    docker build/libs/. -t rover
                '''
            }
        }
        stage('Publish DockerImage')
        {
            steps {
